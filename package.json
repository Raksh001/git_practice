{
  "name": "form-builder",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start1": "ng serve --configuration=dev --host 0.0.0.0 --deploy-url '/w/' --base-href '/w/' --proxy-config=proxy.config.json",
    "start": "ng serve --configuration=dev --host 0.0.0.0 --deploy-url '/w/' --base-href '/w/' --proxy-config=proxy.config.json",
    "build": "ng build --prod",
    "build:staging": "ng build --prod  --aot --configuration=staging --build-optimizer --vendor-chunk --deploy-url /w/ --base-href /w/",
    "build:stagingp": "ng build --prod  --aot --configuration=stagingp --build-optimizer --vendor-chunk --deploy-url /testing/ --base-href /testing/",
    "build:prod": "ng build --prod --configuration=production --build-optimizer --vendor-chunk  --deploy-url /w/ --base-href /w/",
    "build:dev": "ng build --prod --configuration=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.1",
    "@angular/cdk": "^6.4.3",
    "@angular/common": "^6.1.1",
